<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title mt-0" id="myLargeModalLabel">Add New Test</h6>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">


                <div class="mb-3 row">
                    <label for="example-text-input" class="col-sm-2 col-form-label text-end">ID</label>
                    <div class="col-sm-6">
                        <input class="form-control" disabled type="text" value="" id="labId">
                    </div>
                </div>

                <div class="mb-3 row">
                    <label for="example-text-input" class="col-sm-2 col-form-label text-end">Name</label>
                    <div class="col-sm-6">
                        <input class="form-control" type="text" value="" id="name">
                    </div>
                </div>


                <div class="mb-3 row">
                    <label for="example-text-input" class="col-sm-2 col-form-label text-end">Logo</label>
                    <div class="col-sm-6">
                        <input class="form-control" type="file" value="" id="logo">
                    </div>
                </div>

                <div class="mb-3 row">
                    <label for="example-text-input" class="col-sm-2 col-form-label text-end">Description</label>
                    <div class="col-sm-6">
                        <textarea rows="3" placeholder="Description..." class="form-control"></textarea>
                    </div>
                </div>

                <div class="mb-3 row">
                    <label for="example-text-input" class="col-sm-2 col-form-label text-end">IsActve</label>
                    <div class=" col-sm-6">
                        <input type="checkbox" class="custom-control-input" id="horizontalCheckbox" data-parsley-multiple="groups" data-parsley-mincheck="2">
                    </div>
                </div>

                <div class="form-group">
                    <button class="btn btn-de-primary btn-sm text-light px-4 mt-3 mb-0">Save</button>
                    <button class="btn btn-de-danger btn-sm text-light px-4 mt-3 mb-0">Cancel</button>
                </div>

            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>

